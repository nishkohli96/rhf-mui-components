{
  "name": "@nish1896/rhf-mui-components",
  "version": "1.0.3",
  "description": "Reusable mui components for react-hook-form to minimize your time and effort in creating forms",
  "author": "Nishant Kohli",
  "homepage": "https://github.com/nishkohli96/rhf-mui-components#readme",
  "repository": {
    "url": "git+https://github.com/nishkohli96/rhf-mui-components.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc && cp package.json dist/ && cp README.md dist/ && cp .npmignore dist/ && tsc-alias",
    "lint": "eslint --fix ."
  },
  "devDependencies": {
    "@nish1896/eslint-config": "^2.1.1",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.10"
  },
  "peerDependencies": {
    "@mui/icons-material": ">=5",
    "@mui/material": ">=5",
    "react-hook-form": ">=7"
	},
  "exports": {
    "./config": "./config/index.js",
    "./form-helpers": "./form-helpers/index.js",
    "./misc": "./misc/index.js",
    "./misc/*": "./misc/*",
    "./mui": "./mui/index.js",
    "./mui/*": "./mui/*",
    "./mui-pickers": "./mui-pickers/index.js",
    "./mui-pickers/*": "./mui-pickers/*"
  },
  "typesVersions": {
    "*": {
      "./mui": ["mui/index.d.ts"],
      "./mui-pickers": ["mui-pickers/index.d.ts"],
      "./misc": ["misc/index.d.ts"]
    }
  },
  "types": "./types/index.d.ts",
  "keywords": [
    "@nish1896",
    "@nish1896/rhf-mui-components",
    "react-hook-form",
    "material-ui",
    "mui",
    "@mui/material",
    "form builder",
    "form validation",
    "form components",
    "react ui components",
    "react forms library",
    "component library",
    "hookform",
    "@hookform/resolvers",
    "react material components"
  ]
}
