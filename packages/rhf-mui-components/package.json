{
  "name": "@nish1896/rhf-mui-components",
  "version": "3.1.0",
  "description": "A suite of 20+ reusable Material UI components for React Hook Form to minimize your time and effort in creating and styling forms",
  "author": "Nishant Kohli",
  "homepage": "https://rhf-mui-components.netlify.app/",
  "repository": {
    "url": "git+https://github.com/nishkohli96/rhf-mui-components.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc && tsc-alias && cp package.json dist/ && cp README.md dist/ && cp .npmignore dist/",
    "lint": "eslint --fix ."
  },
  "devDependencies": {
    "@nish1896/eslint-flat-config": "^2.0.4",
    "eslint": "^9.25.0",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.15"
  },
  "peerDependencies": {
    "@mui/icons-material": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "@mui/material": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "@mui/x-date-pickers": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "react-hook-form": "^7.0.0"
	},
  "exports": {
    "./package.json": {
      "import": "./package.json",
      "require": "./package.json"
    },
    "./config": {
      "import": "./config/index.js",
      "require": "./config/index.js",
      "types": "./config/index.d.ts"
    },
    "./form-helpers": {
      "import": "./form-helpers/index.js",
      "require": "./form-helpers/index.js",
      "types": "./form-helpers/index.d.ts"
    },
    "./mui": {
      "import": "./mui/index.js",
      "require": "./mui/index.js",
      "types": "./mui/index.d.ts"
    },
    "./mui/*": {
      "import": "./mui/*/index.js",
      "require": "./mui/*/index.js",
      "types": "./mui/*/index.d.ts"
    },
    "./mui-pickers": {
      "import": "./mui-pickers/index.js",
      "require": "./mui-pickers/index.js",
      "types": "./mui-pickers/index.d.ts"
    },
    "./mui-pickers/*": {
      "import": "./mui-pickers/*/index.js",
      "require": "./mui-pickers/*/index.js",
      "types": "./mui-pickers/*/index.d.ts"
    },
    "./misc": {
      "import": "./misc/index.js",
      "require": "./misc/index.js",
      "types": "./misc/index.d.ts"
    },
    "./misc/*": {
      "import": "./misc/*/index.js",
      "require": "./misc/*/index.js",
      "types": "./misc/*/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "./mui": ["./mui/index.d.ts"],
      "./mui/*": ["./mui/*/index.d.ts"],
      "./mui-pickers": ["./mui-pickers/index.d.ts"],
      "./mui-pickers/*": ["./mui-pickers/*/index.d.ts"],
      "./misc": ["./misc/index.d.ts"],
      "./misc/*": ["./misc/*/index.d.ts"],
      "./config": ["./config/index.d.ts"],
      "./form-helpers": ["./form-helpers/index.d.ts"]
    }
  },
  "keywords": [
    "@nish1896",
    "@nish1896/rhf-mui-components",
    "react-hook-form",
    "material-ui",
    "mui",
    "@mui/material",
    "rhf-mui",
    "form builder",
    "form validation",
    "form components",
    "react ui components",
    "react forms library",
    "component library",
    "hookform",
    "@hookform/resolvers",
    "react material components",
    "rhf",
    "rhf mui",
    "rhf-mui components",
    "rhf mui components",
    "react forms"
  ]
}
