{
  "name": "@nish1896/rhf-mui-components-root",
  "version": "1.0.0",
  "description": "Reusable material-ui and other third party components for react-hook-form to minimize your time and effort in creating forms",
  "author": "Nishant Kohli",
  "private": true,
  "workspaces": ["./apps/*", "./packages/*"],
  "scripts": {
    "lib:build": "yarn workspace @nish1896/rhf-mui-components build",
    "lib:lint": "eslint --fix ./packages/rhf-mui-components",
    "demo": "yarn workspace @nish1896/rhf-mui-demo dev",
    "demo:lint": "yarn workspace @nish1896/rhf-mui-demo lint",
    "demo:build": "yarn workspace @nish1896/rhf-mui-demo build",
    "docs": "yarn workspace @nish1896/rhf-mui-components-docs start",
    "docs:lint": "yarn workspace @nish1896/rhf-mui-components-docs lint",
    "docs:build": "yarn workspace @nish1896/rhf-mui-components-docs build",
    "storybook": "yarn workspace @nish1896/rhf-mui-demo storybook",
    "storybook:build": "yarn workspace @nish1896/rhf-mui-demo build-storybook"
  },
  "keywords": [
    "@nish1896",
    "@nish1896/rhf-mui-components",
    "react-hook-form",
    "rhf-mui",
    "reusable rhf components",
    "material-ui",
    "mui"
  ],
  "homepage": "https://github.com/nishkohli96/eslint-config#readme",
  "repository": {
    "url": "git+https://github.com/nishkohli96/eslint-config.git"
  },
  "license": "ISC"
}
